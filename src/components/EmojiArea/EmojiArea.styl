$px = 1 / 30rem;
.emoji-area 
  position: relative;
  width: 100%;
  height: 400 * $px;
  padding: 0 0 0 30 * $px;
  -webkit-overflow-scrolling: touch;
  overflow-x: hidden;
  overflow-y: scroll;
  .emoticon 
    float: left;
    width: 50 * $px;
    height: 50 * $px;
    margin: 15 * $px 30 * $px 15 * $px 0;
    span 
      display: block;
      font-size: 50 * $px;
  .empty 
    clear: both;
    margin-bottom: 20 * $px;
  

